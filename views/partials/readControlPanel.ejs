
<div class="w-100 d-flex px-3 justify-content-between py-2">

  <div class="d-flex">
    <h3 id="type"><%= type %> | </h3>
    <div class="d-flex flex-column mx-3">
      <small id="id" class="mt-1">ID: <%= id %></small>
      <small id="filename" class="mt-1">File Name: <%= filename %></small>
    </div>
  </div>

  <div class="d-flex">
    <button id="deleteBtn" class="btn btn-sm btn-outline-danger mx-2 px-4">
      Delete
    </button>

    <button id="renameModalBtn" class="btn btn-sm btn-outline-primary px-4">
      Rename 
    </button>

    <button id="downloadBtn" class="btn btn-sm btn-outline-primary mx-2 px-4">
      Download 
    </button>

    <button id="setAsDefaultBtn" class="btn btn-sm btn-primary px-4">
      Set as Default
    </button>
  </div>

</div>
